{
  "name": "Live wallpaper for Chrome OS",
  "version": "0.0.1",
  "minimum_chrome_version": "43",
  "permissions": [
    "notifications",
    "videoCapture",
    "wallpaper",
    "http://*/",
    "https://*/"
  ],
  "icons": { "128": "128.png" },
  "app": {
    "background": {
      "scripts": [ "background.js" ]
    }
  },
  "manifest_version": 2
}
